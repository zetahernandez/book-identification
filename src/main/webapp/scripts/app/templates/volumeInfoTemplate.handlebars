<tr id="main--{{unbound volume.id}}"
	class="gb-bookshelf-ordering-holder gb-bookshelf-standard-manage">
	<td><table class="gb-bookshelf-table-container ">
			<tbody>
				<tr>
					<td valign="top" align="center"
						class="coverwrapper gb-bookshelf-hover-card"><a href="">
							{{#if volume.volumeInfo.imageLinks.thumbnail}} 
							<img border="0" src={{unbound volume.volumeInfo.imageLinks.thumbnail}} {{action showVolumeInfo volume}} title={{unbound 	volume.volumeInfo.title}} class="coverthumb hover-card-attach-point" alt={{unbound volume.volumeInfo.title}}> {{/if}}
					</a></td>
					<td><div class="rsi">
							<div>
								<div>
									<h2 class="resbdy">
										<a {{action showVolumeInfo volume}}><span dir="ltr"
											class="gb-volume-title">{{unbound volume.volumeInfo.title}}</span>&lrm;</a>
									</h2>
								</div>
							</div>
							<div class="gb-text-content">
								<div style="line-height: 1.2em; font-size: small">
									<span class="gb-gray-author"><span
										style="color: #676767"><a class="link_aux"
											href="#"><span
												dir="ltr">{{unbound volume.volumeInfo.authors}}</span></a> -
											{{volume.volumeInfo.publishedDate}} -</span></span> {{#if
									volume.volumeInfo.description}}
									<div id="container">
										<div class="more-less">
											<div dir="ltr" class="snippet sa more-block">
												<p>{{volume.volumeInfo.unescapedDescription}}</p>
											</div>
										</div>
									</div>
									{{/if}}
								</div>
							</div>
						</div></td>
				</tr>
			</tbody>
		</table></td>
</tr>