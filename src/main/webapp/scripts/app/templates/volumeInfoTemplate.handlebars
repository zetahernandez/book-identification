<tr id="main--{{unbound book.id}}"
	class="gb-bookshelf-ordering-holder gb-bookshelf-standard-manage">
	<td><table class="gb-bookshelf-table-container ">
			<tbody>
				<tr>
					<td valign="top" align="center"
						class="coverwrapper gb-bookshelf-hover-card"><a
						href="http://books.google.com.ar/books?id=BFlF1TQQ26gC">
						{{#with book.volume_info.image_links.thumbnail}}
						<img
							border="0"
							src={{unbound  book.volume_info.image_links.thumbnail}}
							title={{unbound book.volume_info.title}}
							class="coverthumb hover-card-attach-point"
							alt={{unbound book.volume_info.title}}>
							{{/with}}
					</a></td>
					<td><div class="rsi">
							<div>
								<div>
									<h2 class="resbdy">
										<a {{bindAttr href="book.volume_info.path"}} {{action "linkClicked" on="click"}}><span
											dir="ltr" class="gb-volume-title">{{unbound book.volume_info.title}}</span>&lrm;</a>
									</h2>
								</div>
							</div>
							<div class="gb-text-content">
								<div style="line-height: 1.2em; font-size: small">
									<span class="gb-gray-author"><span
										style="color: #676767"><a class="link_aux"
											href="http://www.google.com.ar/search?tbo=p&amp;tbm=bks&amp;q=inauthor:%22Filippo+di+Pisa%22"><span
												dir="ltr">{{unbound book.volume_info.authors}}</span></a> - {{book.volume_info.publishedDate}} -</span></span>
									<div dir="ltr" class="snippet sa">{{book.volume_info.description}}</div>

								</div>
							</div>
						</div></td>
				</tr>
			</tbody>
		</table></td>
</tr>